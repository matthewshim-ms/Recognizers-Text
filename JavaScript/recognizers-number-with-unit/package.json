{
  "name": "recognizers-text-number-with-unit",
  "version": "1.0.0",
  "description": "recognizers-text-number-with-unit provides robust recognition and resolution of numbers with units expressed in multiple languages. Support for English, Spanish, Chinese and Portuguese.",
  "author": "Microsoft Corp.",
  "license": "MIT",
  "keywords": [
    "nlp",
    "nlp-entity-extraction",
    "entity-extraction",
    "numex",
    "parser-library"
  ],
  "main": "dist/recognizers-text-number-with-unit.umd.js",
  "module": "dist/recognizers-text-number-with-unit.es5.js",
  "typings": "dist/types/recognizers-text-number-with-unit.d.ts",
  "browser": "dist/recognizers-text-number-with-unit.browser.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/Recognizers-Text"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/Recognizers-Text/issues"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prebuild": "npm install --no-save ../recognizers-number/ && rimraf dist && rimraf compiled",
    "build-resources": "ts-node --project tools/resource-generator/ tools/resource-generator/",
    "build": "npm run build-resources && tsc && rollup -c && typedoc --out dist/docs --target es6 --theme minimal src"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.9.0",
    "@types/lodash.escaperegexp": "^4.1.3",
    "@types/lodash.last": "^3.0.3",
    "@types/lodash.max": "^4.0.3",
    "@types/node": "^8.0.20",
    "js-yaml": "^3.9.1",
    "lodash.camelcase": "^4.3.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-node": "^3.3.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "lodash.escaperegexp": "^4.1.2",
    "lodash.last": "^3.0.0",
    "lodash.max": "^4.0.1"
  }
}
