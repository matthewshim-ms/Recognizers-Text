    "@types/lodash.trimend": "^4.5.1",
    "@types/node": "^8.0.20",
    "@types/xregexp": "^3.0.29",
    "js-yaml": "^3.9.1",
    "lodash.camelcase": "^4.3.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-node": "^3.3.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "bignumber.js": "^4.1.0",
    "lodash.escaperegexp": "^4.1.2",
